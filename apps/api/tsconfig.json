{
  "extends": "@ai-router/typescript-config/base.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "lib": [
      "ES2022"
    ],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmit": false,
    "skipLibCheck": true,
    "declaration": false,
    "declarationMap": false
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}