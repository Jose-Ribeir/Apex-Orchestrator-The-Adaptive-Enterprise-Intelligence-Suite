{
  "name": "@ai-router/api",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "start:migrate": "sequelize-cli db:migrate && node dist/index.js",
    "lint": "eslint src --max-warnings 0",
    "check-types": "tsc --noEmit",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:create": "sequelize-cli db:create"
  },
  "dependencies": {
    "better-auth": "^1.4.18",
    "@scalar/express-api-reference": "^0.8.40",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "pg": "^8.18.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "uuid": "^13.0.0",
    "bcrypt": "^6.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@ai-router/eslint-config": "workspace:*",
    "@ai-router/typescript-config": "workspace:*",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.0",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^11.0.0",
    "eslint": "^9.39.1",
    "sequelize-cli": "^6.6.5",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18"
  }
}
