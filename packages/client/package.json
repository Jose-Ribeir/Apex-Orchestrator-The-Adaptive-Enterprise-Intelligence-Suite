{
  "name": "@ai-router/client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "generate": "openapi-ts",
    "update": "curl -s http://localhost:${PORT:-4000}/openapi.json > openapi.json && node -e \"const fs=require('fs');const pkg=require('./package.json');const spec=JSON.parse(fs.readFileSync('openapi.json','utf8'));if(pkg.apiUrl){spec.servers=spec.servers||[];spec.servers[0]={url:pkg.apiUrl,description:spec.servers[0]?.description||'Default'};fs.writeFileSync('openapi.json',JSON.stringify(spec));}\"",
    "upgrade": "pnpm update && pnpm generate",
    "build": "tsup"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./react-query": {
      "types": "./src/@tanstack/react-query.gen.ts",
      "import": "./src/@tanstack/react-query.gen.ts",
      "require": "./src/@tanstack/react-query.gen.ts"
    },
    "./client.gen": {
      "types": "./src/client.gen.ts",
      "import": "./src/client.gen.ts",
      "require": "./src/client.gen.ts"
    }
  },
  "dependencies": {
    "@ai-router/typescript-config": "workspace:*",
    "@tanstack/react-query": "^5.90.20",
    "axios": "^1.13.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@ai-router/eslint-config": "workspace:*",
    "@hey-api/openapi-ts": "^0.77.0",
    "@types/node": "^24.10.10",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "react": ">=18"
  }
}