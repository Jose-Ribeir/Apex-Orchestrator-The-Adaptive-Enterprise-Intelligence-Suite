{
  "name": "@ai-router/client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "generate": "openapi-ts",
    "update": "curl -s http://localhost:${PORT:-4000}/openapi.json > openapi.json",
    "upgrade": "pnpm update && pnpm generate",
    "build": "tsup",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "dependencies": {
    "@ai-router/typescript-config": "workspace:*",
    "@tanstack/react-query": "^5.90.20",
    "axios": "^1.13.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@ai-router/eslint-config": "workspace:*",
    "@hey-api/openapi-ts": "^0.77.0",
    "@types/node": "^24.10.10",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "react": ">=18"
  }
}
