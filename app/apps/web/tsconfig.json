{
  "extends": "@ai-router/typescript-config/react-library.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "Bundler"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src/vite-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}